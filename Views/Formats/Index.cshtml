@model IEnumerable<InventorySys.Models.TblFormat>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FormatName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormatSizeCm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormatActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormatName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormatSizeCm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormatActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FormatId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FormatId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FormatId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
